{
  "expo": {
    "name": "Zona-p",
    "slug": "Zona-p",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#00A9A0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.corpaul.zonap",
      "useNextNotificationsApi": true,
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00A9A0"
      },
      "permissions": ["CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "bluspot",
            "project": "zonap",
            "authToken": "f0821824f2624f409ef7d2c52ffe1bb318d42afe58a84f508c02ab91cbde4610"
          }
        }
      ]
    }
  }
}